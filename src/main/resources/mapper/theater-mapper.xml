<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.chbb.theaketing.feature.theater.mapper.TheaterMapper">
    <select id="findById" parameterType="Long" resultType="com.chbb.theaketing.feature.theater.entity.Theater">
        SELECT  * 
        FROM    theater
        WHERE   id = #{id}
    </select>
</mapper>